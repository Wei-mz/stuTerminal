{
  "name": "terminalclient",
  "version": "0.0.1",
  "description": "Seadee Platform Terminal Client",
  "main": "app.js",
  "dependencies": {
    "bufferutil": "^4.0.3",
    "electron-package": "^0.1.0",
    "electron-packager": "^14.2.1",
    "md5": "^2.2.1",
    "pubsub-js": "^1.6.0",
    "tree-kill": "^1.2.0",
    "utf-8-validate": "^5.0.5",
    "whatwg-fetch": "^2.0.4",
    "ws": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^17.0.2",
    "babel-loader": "^6.4.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.26.4",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^6.10.3",
    "file-loader": "^0.9.0",
    "jest": "^17.0.3",
    "node-gyp": "^3.6.2",
    "node-sass": "^3.13.1",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-redux": "^4.4.9",
    "react-test-renderer": "^15.6.2",
    "redux": "^3.7.2",
    "redux-actions": "^1.2.2",
    "redux-devtools": "^3.3.1",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.9",
    "webpack": "^1.15.0"
  },
  "scripts": {
    "build_debug": "webpack -d",
    "build_release": "webpack -p --config webpack.release.config.js",
    "clean": "rm -fr dist, rm -fr js;",
    "app": "electron .",
    "debug": "npm run build_debug && npm run app",
    "release": "npm run build_release && npm run app",
    "help": "cat README.md",
    "doc": "yuidoc .",
    "pack": "bash script/pack.sh && cd temp && asar pack pack app.asar && cp -f app.asar ../resources",
    "package": "electron-packager ./ studentTerminal --platform=linux --arch=x64 --out=./distww --electron-version=1.4.2 --overwrite --ignore =node_modules",
    "bmod": "curpath=$(pwd); if ! test -f \"$TARGET/binding.gyp\"; then echo 'Error: not a valid module path error!'; exit 1; fi; version=$(electron --version); cd $TARGET; HOME=~/.electron-gyp node-gyp rebuild --target=${version//v} --arch=x64 --dist-url=https://atom.io/download/electron; cd ${curpath}",
    "init": "echo 'do nothing!'",
    "rmdb": "rm -f \"${HOME}/Library/Application Support/terminalclient/database/sqlite.db\"; "
  },
  "repository": {
    "type": "git",
    "url": "https://git.oschina.net/virtualink/ElectronTemplate.git"
  },
  "author": "uforgetmenot",
  "license": "ISC",
  "comment": "TARGET=node_modules/mdns npm run bmod;"
}
